;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Rhythia"
config/version="june19-2024-a"
run/main_scene="res://scenes/init.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="SoundSpacePlus"
config/quit_on_go_back=false
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://assets/images/branding/splash.png"
boot_splash/fullsize=false
config/icon="res://assets/images/branding/icon.png"
config/windows_native_icon="res://icon.ico"
run/delta_sync_after_draw=true
config/autoload_map_registries="test maps|res://test_assets/test_maps.sspmr;built in|res://content/songs/built_in_maps.sspmr"
config/enable_new_content_mgr=true
config/default_menu_target="res://scenes/menu/menu2.tscn"
config/default_menu_target.mobile="res://scenes/menu/menu2.tscn"
config/default_menu_target.editor="res://scenes/menu/menu2.tscn"
config/7zip_binary_path=""
config/7zip_binary_path.Windows="install_dir/util/7z.exe"
config/7zip_binary_path.X11="install_dir/util/7zz"
config/auto_maximize=true
config/auto_maximize.editor=false
config/use_custom_user_dir.Android=false
config/discord_rpc.OSX=false
config/save_dir="user://"
config/save_dir.Android="user://"
config/fov=70.0
config/enable_new_content_mgr.mobile=false
config/discord_rpc.mobile=false
config/replays=true
config/replays.editor=true
config/show_trail_debug=false
config/project_settings_override.standalone="user://override.cfg"
networking/enabled=true
networking/mapdb_api="https://cdn.rhythia.net/index.json"
networking/test_url="https://google.com"
config/discord_rpc.HTML5=false
config/enable_new_content_mgr.HTML5=false
config/disable_native_file_dialogs=false
config/disable_native_file_dialogs.Android=true
config/disable_native_file_dialogs.web=true
config/disable_native_file_dialogs.OSX=true
config/is_using_controller=true
config/discord_rpc=false
config/discord_rpc.editor=false
networking/github_repo="David20122/sound-space-plus"

[audio]

buses/channel_disable_threshold_db=-240.0
buses/channel_disable_time=15.0

[autoload]

Globals="*res://scripts/Globals.gd"
Online="*res://scripts/Online.gd"
Rhythia="*res://scripts/Rhythia.gd"
RQueue="*res://scripts/content/resources/ResourceQueue.gd"
Dance="*res://scripts/cursordance/Dance.gd"
SFXManager="*res://scripts/game/SFXManager.gd"

[debug]

gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false
gdscript/warnings/narrowing_conversion=false
gdscript/warnings/unused_argument=false

[discord_presence]

first_button/label="go to godot's website lol"
settings/change_time_per_screen=true

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/handheld/orientation="sensor_landscape"
window/vsync/vsync_mode=false
window/stretch/mode.mobile="2d"
window/stretch/aspect.mobile="keep_height"

[editor_plugins]

enabled=PackedStringArray()

[gdnative]

